  "homepage": "https://github.com/maurelian/dot-eth-js#readme",
  "dependencies": {
    "ethereum-ens": "^0.4.0",
    "underscore": "^1.8.3",
    "web3": "^0.18.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.4",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "ethereumjs-abi": "^0.6.4",
